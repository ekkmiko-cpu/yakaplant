{
    "version": 2,
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/api",
            "destination": "/api/index.js"
        },
        {
            "source": "/api/:path*",
            "destination": "/api/index.js"
        }
    ],
    "functions": {
        "api/index.js": {
            "includeFiles": "node_modules/sql.js/dist/sql-wasm.wasm"
        }
    }
}
